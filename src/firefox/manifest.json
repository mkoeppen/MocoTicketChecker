{
  "manifest_version": 3,
  "name": "MocoApp Custom JS Injector",
  "version": "1.0",
  "description": "FÃ¼hrt benutzerdefiniertes JS auf mocoapp.com Tabs aus.",
  "permissions": [
    "scripting",
    "tabs",
    "activeTab",
    "webNavigation",
    "storage"
  ],
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "host_permissions": [
    "*://*.mocoapp.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.mocoapp.com/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],
  "action": {
    "default_title": "MocoApp JS Injector",
    "default_popup": "popup.html"
  }
}
